{
  "name": "inco_desktop_v2",
  "version": "1.0.0",
  "description": "proyecto de escritorio de inco",
  "main": "app/index.js",
  "homepage": "./",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:9000 && set URL_HOST=http://localhost:7000 && electron .",
    "electron-hosting": "set ELECTRON_START_URL=http://localhost:9000 && electron .",
    "electron": "electron .",
    "build": "webpack --config ./webpack.config.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "ts:electron": "tsc -w -p tsconfig.electron.json",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^13.1.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-toast-notifications": "^2.0.1",
    "@types/redux-thunk": "^2.1.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.4.1",
    "axios": "^0.19.0",
    "fs-extra": "^8.1.0",
    "is-online": "^8.2.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hook-form": "^4.0.0",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-toast-notifications": "^2.4.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "socket.io-client": "^2.3.0",
    "sqlite": "^3.0.3",
    "sqlite3": "^4.1.1",
    "styled-components": "^4.4.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.22",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.4.0",
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.8",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.1.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "build": {
    "appId": "Inco_desktop",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "NSIS"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
